!!! 5
%html{:lang => "en"}
  %head
    = render 'layouts/head'
  %body
    %header
      = render 'layouts/header'
      = javascript_include_tag "application"

      = render 'shared/flash_messages', locals: { flash: flash}
    .container-fluid
      .row-fluid
        .span3
          = yield :column1
        .span9
          = yield :column2
      .span6.offset3
        = render 'layouts/footer'
    = debug(params) if Rails.env.development?


